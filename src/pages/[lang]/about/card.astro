---
import styles from './personal-info.module.scss'

interface Item {
  label: string
  text: string
}

interface Props {
  item: Item
}

const { item = { label: '', text: '' } } = Astro.props
console.log('Item: ', item)
---

<li class={styles.card}>
  <span>{item.text}</span>
  <p>{item.label}</p>
</li>
